{
  "name": "QML",
  "displayName": "QML",
  "description": "QML language support for Visual Studio Code",
  "version": "1.0.0",
  "publisher": "bbenoist",
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#FFFFFF",
    "theme": "light"
  },
  "license": "MIT",
  "bugs": { "url": "https://github.com/bbenoist/vscode-qml/issues" },
  "homepage": "https://github.com/bbenoist/vscode-qml/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/bbenoist/vscode-qml.git"
  },
  "engines": {
    "vscode": "^0.10.1"
  },
  "categories": [ "Languages", "Linters" ],
  "activationEvents": [
    "onLanguage:qml"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "languages": [{
      "id": "qml",
      "extensions": [".qml",".qmlproject"],
      "configuration": "./qml.configuration.json"
    }],
    "grammars": [{
      "language": "qml",
      "scopeName": "source.qml",
      "path": "./syntaxes/qml.tmLanguage"
    }]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
  },
  "devDependencies": {
    "typescript": "^1.6.2",
    "vscode": "0.10.x"
  },
  "dependencies": {
    "vscode-languageclient": "0.10.x"
  }
}